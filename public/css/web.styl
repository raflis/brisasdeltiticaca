    width 100vw
    height 120vh
    background rgb(0,0,0,1)
    display flex
    justify-content center
    align-items center
    z-index 9999999999999999
    #loader
        border 2px #3F3F3F solid
        border-radius 50%
        border-top-color #FF616A
        width 90px
        height 90px
